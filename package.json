{
  "name": "nic.svelte",
  "version": "0.0.3",
  "description": "NIC",
  "repository": {
    "type": "git",
    "url": "https://github.com/stroyexp/nic.svelte"
  },
  "author": "DVA",
  "homepage": "https://stroyexp.info/",
  "private": true,
  "scripts": {
    "clean": "rimraf .svelte-kit && rimraf build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "node build/index.js",
    "cluster": "node run/cluster.js",
    "dev": "vite dev --port=3000",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate:images": "node run/generate.image-data.js",
    "generate:sitemap": "node run/generate.sitemap.js",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "*",
    "@sveltejs/adapter-node": "*",
    "@sveltejs/adapter-static": "*",
    "@sveltejs/kit": "*",
    "@types/html-minifier-terser": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "cssnano": "*",
    "cssnano-preset-advanced": "*",
    "dotenv": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-svelte3": "*",
    "flowbite": "*",
    "flowbite-svelte": "*",
    "html-minifier-terser": "*",
    "postcss": "*",
    "postcss-import": "*",
    "postcss-load-config": "*",
    "prettier": "*",
    "prettier-plugin-svelte": "*",
    "rimraf": "*",
    "svelte": "*",
    "svelte-check": "*",
    "svelte-heros-v2": "*",
    "tailwindcss": "*",
    "tslib": "*",
    "typescript": "*",
    "vanilla-lazyload": "*",
    "vite": "*",
    "vite-imagetools": "4.0.11",
    "vitest": "*"
  },
  "type": "module"
}
